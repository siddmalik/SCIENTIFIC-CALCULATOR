import math
def add(a,b):
    return  a+b 
def subtract(a,b):
    return a-b
def multiply(a,b):
    return  a*b
def divide(a,b):
    return a/b
def remainder(a,b):
    return  math.remainder(a,b)
def sqrt(a):
    return math.sqrt(a)
def exponentiate(a,b):
    return math.pow(a,b)
def sine(a):
    return math.sin(a)
def cosine(a):
    return math.cos(a)
def tangent(a):
    return math.tan(a)
def rad_to_deg(a):
    return math.degrees(a)
def deg_to_rad(a):
    return math.radians(a)
function_map={
    "1":add,
    "2":subtract,
